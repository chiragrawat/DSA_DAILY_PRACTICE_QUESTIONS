#include<iostream>
using namespace std;

int main()
{
   // where n is node and m is edges
    int n,m;
    cin>>n>>m;
    //create adjeceney list of size number of nodes
    //if weight graph vector<pair<int,int>>adj[n+1];
    vector<int>adj[n+1];
    for(int i=0;i<m;i++)
    {
        int u,v;
        cin>>u>>v;
        /*if weight graph
          //cin>>u>>v>>wt;
        adj[u].push_back({v,wt});
        adj[v].push_back({u,wt});*/
        if  direct graph we use only
        //adj[u].push_back(v);
         if  undirect graph we use only
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    return 0;
}

