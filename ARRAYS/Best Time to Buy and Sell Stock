    int maxProfit(vector<int>& prices) {
        int minprofit=prices[0];
        
        int maxprofit=0;
        for(int i=1;i<prices.size();i++)
        {
            minprofit=min(minprofit,prices[i]);
            int prof=prices[i]-minprofit;
            maxprofit=max(maxprofit,prof);
        }
        return maxprofit;
        
    }
