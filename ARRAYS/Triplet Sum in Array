    bool find3Numbers(int A[], int n, int X)
    {
        //Your Code Here
        sort(A,A+n);
        
        for(int i=0;i<n;i++)
        {
            int low=i+1;
        int high=n-1;
        while(low<high)
        {
            if(A[low]+A[high]+A[i]==X)
            return true;
            if(A[low]+A[high]+A[i]>X)
            {
            high--;
            
            }
            else
            low++;
            
            
        }
        }
        return false;
    }
