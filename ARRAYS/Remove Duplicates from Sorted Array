//CREATE MAP AND CALCULATED FIRST VALUE OF 


    int removeDuplicates(vector<int>& nums) {

       
           map<int,int>mp;
            for(int i=0;i<nums.size();i++)
            {
                mp[nums[i]]++;
                
            }
       int j=0;
        for (auto& it : mp) {
        nums[j]=it.first;
            j++;
            
    }
        return j;
        
    }
