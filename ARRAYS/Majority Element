

  int majorityElement(vector<int>& nums) {
        map<int,int>m;
        int ln;
        int n=nums.size();
        for(int i=0;i<n;i++)
        {
            if(m.find(nums[i])==m.end())
                m[nums[i]]=1;
            else
                m[nums[i]]++;
        }
        for(auto &it:m)
        {
            if(it.second>(n/2))
                ln=it.first;
        }
        return ln;
    }
