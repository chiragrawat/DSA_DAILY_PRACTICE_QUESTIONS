#include<iostream>
using namespace std;
void solve(int *arr,int n)
{
    if(n==0 || n==1)
    return ;

    for(int i=0;i<n;i++)
    {
        if(arr[i]>arr[i+1])
            swap(arr[i],arr[i+1]);
    }
    solve(arr,n-1);
}
int main()
{
    int arr[7]={10,5,3,4,2,1,45};

     solve(arr,6);
     for(int i=0;i<7;i++)
        cout<<arr[i]<<" ";
     return 0;

}
