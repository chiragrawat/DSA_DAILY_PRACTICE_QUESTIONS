class Solution {
public:
    ListNode* swapPairs(ListNode* head) {
        if(head==NULL || head->next==NULL)
            return head;
        ListNode *fast=head->next;
        ListNode *second=fast->next;
        fast->next=head;
        head->next=second;
        head->next=swapPairs(second);
        return fast;
      
        
        
    }
};
