bool isIdentical(Node *r1, Node *r2)
    {
        //Your Code here
        if(r1==NULL && r2==NULL)
        return true;
        if((r1!=NULL && r2==NULL) ||  (r1==NULL && r2!=NULL))
        return false;
        bool isleft=isIdentical(r1->left, r2->left);
        bool isright=isIdentical(r1->right, r2->right);
        bool value=r1->data==r2->data;
        if(isleft && isright && value)
        {
            return 1;
        }
        else
        return 0;
    }
